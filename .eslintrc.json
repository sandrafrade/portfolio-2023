{
    "extends": "next/core-web-vitals",
    "rules": {
        "indent": [
            "error",
            4, 
            {
                "SwitchCase": 1
            }
        ],
        "quotes": [
            "error",
            "single"
        ],
        "no-unused-vars": [
            "error",
            {
                "vars": "all",
                "varsIgnorePattern": "IntlMessages"
            }
        ]
    }
}